<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      xmlns="http://javafx.com/javafx/8">
   <padding>
      <Insets bottom="12.0" left="12.0" right="12.0" top="12.0"/>
   </padding>
   <HBox prefWidth="200.0" spacing="17.0">
      <TitledPane collapsible="false" prefHeight="200.0" prefWidth="200.0" text="All points">
         <ListView id="lvAllPoints" prefHeight="200.0" prefWidth="200.0"/>
      </TitledPane>
      <TitledPane collapsible="false" prefHeight="200.0" prefWidth="200.0" text="Area points">
         <ListView id="lvAreaPoints" prefHeight="200.0" prefWidth="200.0"/>
      </TitledPane>
   </HBox>
   <Label id="lblMin">
      <VBox.margin>
         <Insets bottom="6.0" top="17.0"/>
      </VBox.margin>
   </Label>
   <Label id="lblMax"/>
</VBox>
